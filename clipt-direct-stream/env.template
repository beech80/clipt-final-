# Production Cloudflare + Firebase Configuration
# Rename this file to .env and fill in your actual credentials

# Cloudflare Stream Configuration
REACT_APP_CF_ACCOUNT_ID=your_cloudflare_account_id
REACT_APP_STREAM_VIDEO_ID=your_video_id      # the Cloudflare "Live Input" Video ID

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
REACT_APP_FIREBASE_DATABASE_URL=https://your_project_id.firebaseio.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id
